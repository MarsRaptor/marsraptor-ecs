{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [
            "./.docs/"
        ],
        "includePattern": ".+\\.js?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],    
    "markdown": {
        "parser": "gfm",
        "hardwrap": false
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "private": false,
        "lenient": true,
        "destination": "./docs"
    }
}